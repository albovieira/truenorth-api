{
  "name": "truenorth-api",
  "version": "1.0.0",
  "description": "Truenorth API",
  "prisma": {
    "seed": "npx ts-node prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "cross-env NODE_ENV=local npx ts-node src/api.ts ",
    "lint": "eslint --ext .ts .",
    "db:init": "npm run db:generate && npm run db:reset",
    "db:generate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:create-client": "npx prisma generate",
    "db:format": "npx prisma format",
    "test": "jest"
  },
